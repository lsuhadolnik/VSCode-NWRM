{
  "name": "dynamics-crm-webresources",
  "displayName": "Dynamics CRM Web Resources Manager",
  "description": "Manage Dynamics CRM Web Resources from VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dynamicsCrm.connect"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dynamicsCrm.connect",
        "title": "Dynamics CRM: Connect"
      }
    ]
  },
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "test": "npm run build"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
